{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://gatsby-starter-portfolio-minimal-theme.netlify.app/",
    "title": "Lamia Arts Gallery",
    "titleTemplate": "%s · Collection",
    "description": "This is a collection of the paintings made by Lamia Mekki Berrada.",
    "author": "Konstantin Münster",
    "avatar": "../content/images/gatsby.png",
    "bio": "This biography will be used in the footer area of an article and can contain some more details about the author.",
    "social": {
      "behance": "https://behance.com",
      "github": "https://github.com/konstantinmuenster",
      "medium": "https://konstantinmuenster.medium.com",
      "linkedin": "https://www.linkedin.com/in/lamiaberrada/",
      "mail": "mailto:mblamia@gmail.com"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "Lamia Arts Gallery" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "Collections", "url": "/#collections" }
      ],
      "footer": [
        { "label": "Women Print", "url": "/print-collection" },
        { "label": "Women Original", "url": "/original-collection" },
        { "label": "Abstract", "url": "/abstract-collection" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": false,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
